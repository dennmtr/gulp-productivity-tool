{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "sourceMap": true,
    "strict": true,
    "lib": [
      "esnext"
    ],
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ],
  "ts-node": {
    "compilerOptions": {
        "module": "commonjs"
    }
}
}
